{
  "compilerOptions": {
    "target": "ES2017",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "typeRoots": ["./node_modules/@types"],
    "types": [
      "babel__core",
      "babel__generator",
      "babel__template",
      "babel__traverse",
      "eslint",
      "graceful-fs",
      "html-minifier-terser",
      "istanbul-lib-coverage",
      "istanbul-lib-report",
      "istanbul-reports",
      "parse-json",
      "prettier",
      "stack-utils",
      "yargs"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": [
        "./*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".next/types/**/*.ts",
    "next-env.d.ts",
    "out/types/**/*.ts"
, "app/layout.tsx", "app/page.jsx"  ],
  "exclude": [
    "node_modules"
  ]
}
